def solution(absolutes, signs):
    total_sum = 0
    for i in range(len(absolutes)):
        if signs[i]:
            total_sum += absolutes[i]
        else:
            total_sum -= absolutes[i]
    return total_sum

absolutes = [4, 7, 12]
signs = [True, False, True]
print(solution(absolutes, signs))
